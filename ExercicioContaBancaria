package projconta;
public class Conta {
//atributos

    int numero;
    String titular;
    double saldo;

    //metodos    
    
}

class Principal{
    
    public static void main(String[] args) {
        
        Conta c1;
        c1 = new Conta();
        
        c1.titular = "Maria";
        c1.numero = 1234;;
        c1.saldo = 1000.0;
        
        System.out.println("A conta "+(c1.numero)+ " pertence ao(a) " + (c1.titular)+ " e tem R$ "+ (c1.saldo)+" de saldo.");
    }
}
